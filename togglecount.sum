Private Sub ToggleIt()
 
    On Error Resume Next

    If ActiveSheet.PivotTables("PivotTable2").PivotFields("Sum of SP/UOM").Function = xlSum Then
        With ActiveSheet.PivotTables("PivotTable2").PivotFields("Sum of SP/UOM")
            .Function = xlCount
        End With
    Else

        With ActiveSheet.PivotTables("PivotTable2").PivotFields("Count of SP/UOM")
            .Function = xlSum
        End With
    End If
End Sub

